function getDetails(){
    setTimeout(() => {
        console.log("DETAILS")
    }, 2000);
}

function getUser(){
    setTimeout(() => {
        console.log("USER");
        //getDetails(); // Normally calling the function
    }, 3000);
}

getUser();




